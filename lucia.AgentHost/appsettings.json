{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RouterExecutor": {
    "ConfidenceThreshold": 0.7,
    "MaxAttempts": 2,
    "Temperature": 0.1,
    "MaxOutputTokens": 512,
    "IncludeAgentCapabilities": true,
    "IncludeSkillExamples": false
  },
  "AgentExecutorWrapper": {
    "Timeout": "00:00:30",
    "HistoryLimit": 20
  },
  "ResultAggregator": {
    "DefaultFallbackMessage": "I encountered an issue processing your request. Please try again.",
    "EnableNaturalLanguageJoining": true
  },
  "Redis": {
    "TaskTtlHours": 24,
    "ConnectRetryCount": 3,
    "ConnectTimeout": 5000
  }
}
